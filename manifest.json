{
    "manifest_version": 2,
    "name": "Github Events Viewer",
    "version": "0.0.1",
    "description": "Let browser show github users and repos activity events",
	"icons": {
		"16": "img/icon16.png",
		"48": "img/icon32.png",
		"128": "img/icon32.png"
	},
    "content_scripts": [
        {
            "matches": [
                "http://github.com/*",
                "https://github.com/*"
            ],
            "js": ["inject/js/main.js"],
            "css": ["inject/css/main.css"],
            "run_at": "document_start"
        }
    ],
    "permissions": []
}